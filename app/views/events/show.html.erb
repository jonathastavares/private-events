<p id="notice"><%= notice %></p>

<p>
  <strong>Event-date:</strong>
  <%= @event.event_date %>
</p>

<h2> Attendees </h2>
<table>
  <tbody>
    <% @event.attendees.each do |attendee| %>
      <tr>
        <td><%= link_to attendee.email, profile_path(attendee.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= show_registration_button(@event) %>

<%= link_to 'Back', events_path %>
